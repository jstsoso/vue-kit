{
  "name": "vue-kit",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn && cross-env APP_ENV=development webpack-dev-server --progress",
    "staging": "yarn && cross-env APP_ENV=staging webpack",
    "build": "yarn && cross-env APP_ENV=production webpack",
    "lint": "yarn && stylelint --max-warnings 0 'src/**/*.{vue,html,css,less,scss,sass}' && eslint --max-warnings 0 'src/**/*.{js,vue}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    },
    "commit-msg": "echo $HUSKY_GIT_PARAMS"
  },
  "lint-staged": {
    "src/**/*.{vue,html,css,less,scss,sass}": "stylelint",
    "src/**/*.{js,vue}": "eslint",
    "./*.js": "eslint"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.11.2",
    "@sentry/browser": "^5.22.3",
    "@sentry/integrations": "^5.22.3",
    "axios": "^0.20.0",
    "dayjs": "^1.8.34",
    "element-ui": "^2.13.2",
    "lodash": "^4.17.20",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.12",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-lodash": "^3.3.4",
    "big.js": "^5.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.2",
    "eslint": "^7.8.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-vue": "^7.0.0-beta.3",
    "file-loader": "^6.1.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^4.2.5",
    "less": "^3.12.2",
    "less-loader": "^7.0.0",
    "lint-staged": "^10.2.13",
    "mini-css-extract-plugin": "^0.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^3.0.0",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.1",
    "stylelint": "^13.7.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^2.1.0",
    "vue-eslint-parser": "^7.1.0",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
